<% if result.json.key?('agent_genders') && result.json['agent_genders'].any? %>
  <h2><%= I18n.t('agent_gender._plural') %></h2>
  <ul>
  <% result.json['agent_genders'].each do |n| %>
    <li><%= I18n.t("enumerations.gender.#{n['gender']}", default: n['gender']) %></li>
  <% end %>
  </ul>
<% end %>